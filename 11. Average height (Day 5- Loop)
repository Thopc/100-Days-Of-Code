student_height = input("Input a list of student heights ").split()
print(student_height)
# this is for change number in the list from string to number:
for n in range(0, len(student_height)):
    student_height[n] = int(student_height[n])
    print(student_height[n])

total_height = 0
for height in student_height:
    total_height +=height
print(total_height)

total_student = 0
for number in student_height:
    total_student += 1
print(total_student)
average_height = round(total_height / total_student)
print(average_height)


