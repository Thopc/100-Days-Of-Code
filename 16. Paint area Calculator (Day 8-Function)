import math
def paint_cal(height, width, cover):
    nums_can = (height * width) / cover
    round_nums_can = math.ceil(nums_can)
    print(f"you will need{round_nums_can} paint can")
test_h = int(input("please input height(m): "))
test_w = int(input("please input width(m): "))
coverage = 5
paint_cal(height=test_h, width=test_w, cover=coverage)
